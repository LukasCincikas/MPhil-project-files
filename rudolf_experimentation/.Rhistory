PERCENTAGE <- 100
INDIVIDUALS <- 25
INDIVIDUALS[1] <- 25
INDIVIDUALS[2] <- 43
GROUPS <- 2
#Initialisation of some variables:
overall_trial <- NA
block <- NA
trial_within_block <- NA
ascending_bets <- NA
starting_points <- NA
finishing_points <- NA
n_red <- NA
chose_red <- NA
proportion_staked <- NA
chosen_bet_index <- NA
target_was_behind_red <- NA
subject_won <- NA
subject_name <- NA
group_name <- NA
condition_name <- NA
segment_number <- NA
temp_data_tracker <- NA
gamble_type_tracker <- NA
file_number <- NA
accum <- 0
DataModifier <- function(
file_name
) {
temp_data <- read.csv(file_name)
temp_data_tracker <- length(temp_data$block)
block_tracker <- 0
#The main activity loop is here
for(i in 1:temp_data_tracker)
{
temp_data$percentage.staked[i] <- temp_data$percentage.staked[i] / PERCENTAGE
#Replaces "subject.won" column into binary digits
if        (temp_data$subject.won[i] == "yes") {
temp_data$subject.won[i] <- 1
} else if (temp_data$subject.won[i] == "no"){
temp_data$subject.won[i] <- 0
} else {
print("Error in 'subject.won' column!")
break
}
#Replaces "left.won" column into binary digits
if(temp_data$left.won[i] == "yes") {
temp_data$left.won[i] <- 1
} else if (temp_data$left.won[i] == "no") {
temp_data$left.won[i] <- 0
} else {
print("Error in 'left.won' column!")
break
}
#Replaces "left.colour.chosen" column into binary digits
if(temp_data$left.colour.chosen[i] == "yes") {
temp_data$left.colour.chosen[i] <- 1
} else if (temp_data$left.colour.chosen[i] == "no") {
temp_data$left.colour.chosen[i] <- 0
} else {
print("Error in 'left.colour.chosen' column!")
break
}
#Fixes the 'block' column. Weird structure due to real data only counting blocks as 1 or 2, when need 1-4
if(is.na(temp_data$block[i]) == TRUE) {
temp_data$block[i] <- block_tracker
} else {
block_tracker <- block_tracker + 1
temp_data$block[i] <- block_tracker
}
#Fixes the 'gamble.type' column
if(temp_data$gamble.type[i] == "") {
temp_data$gamble.type[i] <- gamble_type_tracker
} else {
gamble_type_tracker <- temp_data$gamble.type[i]
}
#Changes ''gamble.type' into binary
if(temp_data$gamble.type[i] == "Ascending") {
temp_data$gamble.type[i] <- 1
} else if (temp_data$gamble.type[i] == "Descending") {
temp_data$gamble.type[i] <- 0
} else {
print("Error when converting gamble type into binary")
break
}
}
return(temp_data)
}
BetIndex <- function(percentage) {
tracker <- length(percentage)
holder <- 0
for(i in 1:tracker) {
holder[i] <- ForwardStake(percentage[i])
}
return(holder)
}
ForwardStake <- function(percentage) {
if(percentage == 0.05) {
return(1)
} else if (percentage == 0.25) {
return(2)
} else if (percentage == 0.5) {
return(3)
} else if (percentage == 0.75) {
return(4)
} else if (percentage == 0.95) {
return(5)
}
}
BlockTrialGenerator <- function(block) {
tracker <- length(block)
trial_within_block <- 0
current_block <- 0
for (i in 1:tracker) {
if (current_block == block[i]) {
trial_within_block[i] <- trial_within_block[i-1] + 1
} else {
current_block <- current_block + 1
trial_within_block[i] <- 1
}
}
return(trial_within_block)
}
